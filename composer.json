{
    "name": "philadelphia/philadelphia",
    "type": "project",
    "description": "The PhiladelPhia is a FrameWork for PHP",
    "keywords": [
        "framework",
        "philadelphia"
    ],
    "license": "MIT",
    "homepage": "https://philadelphia.com",
    "authors": [
        {
            "name": "Fernando Fernandez",
            "email": "frfernandez1996@gmail.com"
        }
    ],
    "scripts": {
        "lint": "php server.php -l",
        "start": "php -S localhost:8080 server.php",
        "start-public": "php -S 0.0.0.0:8081 -t ./public"
    },
	"autoload": {
		"classmap": [
			"app"
		]
    },
    "require": {
        "php": ">=5.6.9",
        "philadelphia/app": "1.0.0",
        "philadelphia/router": "1.0.0",
        "philadelphia/database": "1.0.0",
        "philadelphia/helpers": "1.0.0",
        "firebase/php-jwt": "^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "process-timeout": 0
	},
    "minimum-stability": "dev"
}
